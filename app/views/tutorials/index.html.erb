<%=link_to "Back", root_path %>
<br><br>

<div class="row">
  <div class="col-lg-6">

    <h1>Tutorials</h1>
    <br>
    <table class="table table-striped">
      <thead>
        <tr>
          <th scope="col">Tutorial Name</th>
          <th scope="col">ID</th>
        </tr>
      </thead>
      <tbody>
        <% @tutorials.each do |tutorial| %>
          <tr>
            <td><%= link_to tutorial.name, tutorial_url(tutorial.id) %></td>
            <td><%= tutorial.id %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

  </div>
</div>
